{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@council/tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
    },
  },
}
