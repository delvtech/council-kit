{
  "name": "council-sdk-starter",
  "version": "0.0.1",
  "description": "Boilerplate to get started using the council-sdk fast!",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "alias": {
    "src": "./src"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "example": "ts-node -r dotenv/config -r tsconfig-paths/register src/example/index.ts",
    "watch:example": "nodemon -r dotenv/config -r tsconfig-paths/register src/example/index.ts"
  },
  "dependencies": {
    "@council/sdk": "*",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@council/eslint-config": "*",
    "@council/prettier-config": "*",
    "@council/tsconfig": "*",
    "dotenv": "^16.0.3",
    "parcel": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
