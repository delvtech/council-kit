{
  "name": "@delvtech/council-core",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "docs": "typedoc",
    "watch:docs": "typedoc --watch",
    "example": "ts-node -r dotenv/config -r tsconfig-paths/register src/example/index.ts",
    "watch:example": "nodemon -r dotenv/config -r tsconfig-paths/register src/example/index.ts",
    "test": "vitest run",
    "test:watch": "vitest --reporter=verbose",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@council/evm-client": "*"
  },
  "dependencies": {
    "@council/artifacts": "*",
    "big.js": "^6.2.1",
    "fast-json-stable-stringify": "^2.1.0",
    "lru-cache": "^10.2.0"
  },
  "devDependencies": {
    "@council/eslint-config": "*",
    "@council/evm-client": "*",
    "@council/tsconfig": "*",
    "@jest/globals": "^29.7.0",
    "@types/big.js": "^6.2.2",
    "abitype": "^1.0.0",
    "dotenv": "^16.4.1",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
