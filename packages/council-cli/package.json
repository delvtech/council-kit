{
  "name": "@delvtech/council-cli",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "dist/council.js",
  "bin": "dist/council.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "council": "node -r dotenv/config dist/council.js",
    "dev": "NODE_ENV=development tesm-node -r dotenv/config src/council.ts",
    "prod": "tesm-node -r dotenv/config src/council.ts",
    "build": "rimraf dist && tsc",
    "watch": "rimraf dist && tsc --watch"
  },
  "dependencies": {
    "@council/artifacts": "*",
    "@delvtech/council-viem": "*",
    "ajv": "^8.12.0",
    "cfonts": "^3.2.0",
    "cli-table": "^0.3.11",
    "clide-js": "^0.1.2",
    "clide-plugin-command-menu": "^0.0.7",
    "colors": "^1.4.0",
    "json-schema-typed": "^8.0.1",
    "merkletreejs": "^0.3.11",
    "node-fetch-native": "^1.6.1",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@council/eslint-config": "*",
    "@council/tsconfig": "*",
    "@types/cli-table": "^0.3.4",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "@types/signale": "^1.4.7",
    "dotenv": "^16.4.1",
    "hardhat": "^2.19.5",
    "rimraf": "^5.0.5",
    "tesm-node": "^1.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "viem": "^2.7.3"
