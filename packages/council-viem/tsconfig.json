{
  "extends": "@council/tsconfig/base.json",
  "include": ["src", "test"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "src/*": ["src/*"],
    },
    "resolveJsonModule": true,
  },
}
