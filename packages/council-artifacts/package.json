{
  "name": "@council/artifacts",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "artifacts"
  ],
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:src": "sh scripts/build-src.sh src",
    "build": "npm run build:src && tsc --build --force"
  },
  "devDependencies": {
    "@council/eslint-config": "*",
    "@council/tsconfig": "*",
    "@openzeppelin/contracts": "^4.2.0",
    "hardhat": "^2.19.4",
    "typescript": "^5.3.3"
  }
}
