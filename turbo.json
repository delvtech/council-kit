{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:packages": {
      "outputs": ["dist/**"]
    },
    "council-ui#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": [
        "NEXT_PUBLIC_MAINNET_ALCHEMY_KEY",
        "NEXT_PUBLIC_GOERLI_ALCHEMY_KEY",
        "NEXT_PUBLIC_LOCAL_RPC_URL"
      ]
    },
    "@council/sdk#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["PROVIDER_URI", "NODE_ENV"]
    },
    "@council/deploy#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["GOERLI_DEPLOYER_PRIVATE_KEY", "GOERLI_URI"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "format": { "outputs": [] },
    "format:check": { "outputs": [] }
  }
}
