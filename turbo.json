{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "council-ui#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": [
        "NEXT_PUBLIC_ALCHEMY_KEY",
        "NEXT_PUBLIC_CHAIN_ID",
        "NEXT_PUBLIC_LOCAL_RPC_URL"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "format": { "outputs": [] },
    "format:check": { "outputs": [] }
  }
}
